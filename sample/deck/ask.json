{
	"_名词":[
		"::5::{%_生物名词}","::3::{%_化学名词}","::5::{%人名}","::10::{%现代名词}","{%_学科}",
		"::5::{%_数学名词}","{%形容词}{%_名词}","::20::{%常规名词}"
	],
		"_学科":[
			"语文","数学","英语","物理","化学","生物","地理","政治",
			"历史","日语","俄语","音乐","美术","体育","心理","科学","信息","技术","法语"
		],
		"常规名词":[
			"扑克","书","手纸","火车","汽车","飞机","::10::{%食物}",
			"键盘","手机","显示器","防盗门",
			"灭火器","平角裤","桌子","椅子","::5::{%景物}","::5::{%时间名词}"
		],
			"景物":[
				"阳光","月光","彩虹","花海","极光","朝霞","霞光","黄昏","晚霞"
			],
			"食物":[
				"炒饭","冰激凌","火锅","烤肉","米饭","粥",
				"汤","炸鸡","薯条","汉堡","面包","面条","饺子",
				"汤圆","饼","馅饼","春卷","鸡肉卷","重庆小面","巧克力","糖果","苹果"
			],
			"时间名词":[
				"黄昏","清晨","中午","下午","午夜","凌晨","深夜",
				"上午","傍晚","午后","白天","晚上"
			],
		"_现代名词":[
			"老婆","小姨子",
			"老公","小叔子","键盘","手机","显示器","防盗门",
			"灭火器","主播","胖次","小龙虾","铜锣烧","切糕","手手","脚脚","瓜","火锅","炒饭",
			"书","手纸","主播","电影","报纸","动物世界","新闻联播"
		],
		"_数学名词":["{%几何}"],
			"大写数字":["一","二","三","四","五","六","七","八","九","十"],
			"几何":[
				"椭圆","圆","矩形","平行四边形","正方形","[1d10+2]边形",
				"圆柱","圆锥","[1d10+2]棱柱","[1d10+2]棱锥","长方体",
				"离心率","半径","直径","体积","面积","周长"
			],
		"_化学名词":[
			"{%_物质名词}","{%实验仪器}"
		],
			"天干":["甲","乙","丙","丁","戊","己","庚","辛","壬","癸"],
			"基础物质名":["烷","醇","烯","酮","酸","醛","炔"],
			"_物质名词":["{%天干}{%基础物质名}","{%天干}{%大写数字}{%基础物质名}","[1d5]-{%天干}{%基础物质名}"],
			"实验仪器":[
				"试管","烧杯","玻璃棒","广口瓶","滴定管","酸式滴定管",
				"碱式滴定管","量筒","离心管","酒精灯","烧杯","烧杯","烧杯"
			],
		"_生物名词":[
			"{%_植物名词}","{%_动物名词}","{%细胞}","{%器官}","{%疾病}","{%_生物物质}"
		],
			"_植物名词":["杨树","柳树","土豆","豌豆","藤蔓","草","花","小麦","西瓜","香蕉","拟南芥"],
			"_动物名词":["牛","羊","人","猪","苍蝇","蚊子","猫","老鼠","狗","鸟"],
			"_生物物质":["ATP","DNA","RNA","腺嘌呤","鸟嘌呤","胞嘧啶","胸腺嘧啶","尿嘧啶","氨基酸","蛋白质"],
			"_生物个体":["{%_植物名词}","{%_动物名词}"],
			"细胞":[
					"细胞","线粒体","细胞器","叶绿体","染色体","端粒","DNA","高尔基体",
					"细胞壁","染色体","染色体","染色体","内质网","{细胞}膜","{%_名词}细胞"
			],
			"器官":[
					"心","大脑","小脑","脑干","皮肤","手","脚","手臂",
					"大腿","小腿","骨骼","尾巴","足","眼","鼻","耳"
			],
			"疾病":[
					"色盲","色弱","失明","21三体","糖尿病","血友病","癫痫","白化病"
			],
		"人名":[
			"{%现代混乱名}","{%历史人物名}","{%通用名称}"
		],
			"通用名称":[
				"::5::{%_学科}老师","校长",
				"{%人名}家邻居","野狗","偶遇的野猫","{%人名}的同事","{%人名}的老板",
				"{%人名}公司楼下卖煎饼的大叔","在快手上认识的老哥","在抖音上认识的老姐",
				"在微信上认识的老弟","在陌陌上认识的老妹","沙雕小学生","沙雕网友","传奇调查员"
			],
			"现代混乱名":[
				"马云","马化腾","王思聪","赵四","刘能","谢广坤",
				"马里奥","JOJO","机器猫","Nico","谢大脚","马大帅",
				"马里奥","蜘蛛侠","奥巴马","特朗普","乔布斯","李三光",
				"如花","八两金","郭森斯坦达","冯小肛","孙笑川","EDC",
				"桥本环奈","广末凉子","新垣结衣","Lady Gaga","罗志祥",
				"M-zone人","蔡徐坤","哈利波特","伏地魔","巫妖王",
				"Uzi","大湿的学徒","RH","汉尼拔","吴亦凡"
			],
			"历史人物名":[
				"孔子","孔明","曹操","赵云","汉高祖","忽必烈","李白",
				"杜甫","贺知章","刘邦","吕不韦","赵飞燕","西施","慈禧",
				"褒姒","诸葛亮","徐君宝","陈寅格","李香君","徐志摩",
				"孙膑","孙武","张良","商鞅","吕尚","嵇康","阮籍",
				"山涛","向秀","王戎","刘伶","阮咸","安重荣","石敬瑭","徐知浩","朱友贞",
				"荆浩","冯道","刘煦","李升","刘龚","李昊","刘旻","辛路","王峻","郭威","王朴","柴荣","徐熙"
			],
	"动词":[
		"::10::{%基础动词}","{%_生物动词}","{%数学动词}","{%化学动词}"
	],
		"基础动词":[
			"打","踢","跑","跳","删","摸","剪","吃",
			"看","砍","走","揍","玩","拿","扔","甩","摔","怼","取",
			"废除","哭","笑","痛哭","翻","洗","吸","舔"
		],
		"_生物动词":[
			"自交","杂交","测交","发酵","有丝分裂","呼吸",
			"交配","注射","培养","对照实验"
		],
		"数学动词":[
			"计算","求","连接",
			"减","加","求导","乘","除","乘方"
		],
		"化学动词":[
			"称量","搅拌","混匀",
			"加热","定量转移","振荡","称取","冷却","点燃"
		],
	"ad":[
		"开心","伤心","疯狂","璀璨","可爱","痛苦","痴狂",
		"大声","嘶哑","平静","小声","静谧","干净","偶遇","贪婪"
	],
	"形容词":[
		"{%ad}的","{%颜色}的","{%_名词}的"
	],
		"_方向":[
			"{%_基础方向}","{%_基础方向}偏{%_基础方向}[1d90]度"
		],
			"_基础方向":["东","南","西","北"],
	"副词":[
		"{%ad}地"
	],
		"颜色":["红色","绿色","橙色","白色","黑色","黄色","蓝色","紫色","彩虹色","无色"],

	"句式":[
		"{%把被句}",
		"{%位置}，{%_名词}在{%副词}{%动词}",
		"到处是{%_名词}到处是{%_名词}到处是{%_名词}！！！",
		"{%_名词}在{%副词}{%动词}{%_名词}",
		"你知道吗，{%_名词}和{%_名词}正{%位置}{%副词}{%动词}！",
		"怎样才能让{%_名词}和{%_名词}{%位置}{%副词}{%动词}呢？",
		"请{%副词}{%动词}{%_名词}。",
		"{%_名词}和{%_名词}{%副词}{%动词}，请问这样做的结果是什么？",
		"为什么会有[1d10]个{%_名词}在{%形容词}的家里{%动词}{%_名词}？",
		"会{%_学科}的{%食物}在{%副词}{%_生物动词}！",
		"{%人名}因为{%位置}{%副词}{%基础动词}了[1d10]株{%生物名词}被{%基础动词}了。",
		"患{%疾病}、{%形容词}{%数学名词}在教{%食物}{%_学科}。",
		"每天{%时间名词}，我都是不{%形容词}{%_名词}。"
	],
		"位置":["在{%_名词}里","在距离{%_名词}{%_方向}[1d100]km的地点","在{%_方向}方向"],
		"把被句":["{%_名词}把{%_名词}{%基础动词}了。","{%_名词}被{%_名词}{%基础动词}了。"],
	"ask":["{%句式}"],
	"取名":["{%形容词}{%_名词}","{%_名词}，{%_名词}与{%_名词}","{%时间名词}时的{%_名词}","{%动词}"]
}
